<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <servlet>
        <servlet-name>students</servlet-name>
        <servlet-class>ru.itis.inform.servlets.StudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>students</servlet-name>
        <url-pattern>/students</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>auto</servlet-name>
        <servlet-class>ru.itis.inform.servlets.AutoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>auto</servlet-name>
        <url-pattern>/auto</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>studentsAddForm</servlet-name>
        <servlet-class>ru.itis.inform.servlets.StudentAddServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>studentsAddForm</servlet-name>
        <url-pattern>/studentsAddForm</url-pattern>
    </servlet-mapping>

</web-app>
